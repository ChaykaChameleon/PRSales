program PrSales;

uses
  Vcl.Forms,
  uSales in 'uSales.pas' {FmSales},
  uSalesChange in '..\Sales\uSalesChange.pas' {fmSalesChange},
  uSalesContentsChange in '..\Sales\uSalesContentsChange.pas' {fmSalseContentsChange};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFmSales, FmSales);
  Application.CreateForm(TfmSalesChange, fmSalesChange);
  Application.CreateForm(TfmSalseContentsChange, fmSalseContentsChange);
  Application.Run;
end.
